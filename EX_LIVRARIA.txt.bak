/*Q1) Trazer todos os dados. */
SELECT * FROM LIVROS; 

/*Q2) Trazer o nome do livro e o nome da editora. */
SELECT LIVRO, EDITORA
FROM LIVROS;

/*Q3) Trazer o nome do livro e a UF dos livros publicados por autores do sexo masculino. */
SELECT LIVRO, UF, AUTOR FROM LIVROS
WHERE SEXO_AUTOR LIKE '%M';

/*Q4) Trazer o nome do livro e o número de páginas do livros publicados por autoras do sexo feminino. */
SELECT LIVRO, NUM_PAGINAS, AUTOR FROM LIVROS
WHERE SEXO_AUTOR LIKE '%F';

/*Q5) Trazer os valores dos livros das editoras de SP */
SELECT LIVRO, VALOR FROM LIVROS
WHERE UF = 'SP';
 
/*Q6) Trazer os dados dos autores do sexo masculino que tiveram livros publicados por São Paulo ou Rio de Janeiro (DESAFIO). */
SELECT AUTOR, SEXO_AUTOR
FROM LIVROS
WHERE SEXO_AUTOR = 'M'
AND UF = 'SP' OR UF = 'RJ';



